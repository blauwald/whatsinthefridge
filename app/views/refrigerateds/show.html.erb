<div class="page-header">
  <h3>Refrigerated Groceries</h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Image</dt>
      <dd><%= @refrigerated.image_of_grocery %></dd>
      <dt>Name</dt>
      <dd><%= @refrigerated.grocery_name %></dd>
      <dt>Purchase date</dt>
      <dd><%= @refrigerated.date_of_purchase %></dd>
      <dt>Experation date<</dt>
      <dd><%= @refrigerated.date_of_experation %></dd>
      <dt>Who bought this</dt>
      <dd><%= @refrigerated.user_id %></dd>
      <dt>comments</dt>
      <dd><%= @refrigerated.date_of_experation %></dd>


    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/refrigerateds" class="btn btn-default">
        Back
      </a>
      <a href="/refrigerateds/<%= @refrigerated.id %>/edit" class="btn btn-default">
        Edit
      </a>
      <a href="/delete_refrigerated/<%= @refrigerated.id %>" class="btn btn-default" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
