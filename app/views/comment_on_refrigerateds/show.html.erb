<div class="page-header">
  <h3>Comment On Refrigerated #<%= @comment_on_refrigerated.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>User</dt>
      <dd><%= @comment_on_refrigerated.user_id %></dd>

      <dt>Body</dt>
      <dd><%= @comment_on_refrigerated.body %></dd>

      <dt>Refrigerated</dt>
      <dd><%= @comment_on_refrigerated.refrigerated_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/comment_on_refrigerateds" class="btn btn-primary">
        Back
      </a>
      <a href="/comment_on_refrigerateds/<%= @comment_on_refrigerated.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_comment_on_refrigerated/<%= @comment_on_refrigerated.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
