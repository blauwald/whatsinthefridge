<div class="page-header">
  <h3>My Grocery #<%= @my_grocery.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Frozen</dt>
      <dd><%= @my_grocery.frozen_id %></dd>

      <dt>Refrigerated</dt>
      <dd><%= @my_grocery.refrigerated_id %></dd>

      <!-- <dt>User</dt>
      <dd><%= @my_grocery.user_id %></dd> -->

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/my_groceries" class="btn btn-primary">
        Back
      </a>
      <a href="/my_groceries/<%= @my_grocery.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_my_grocery/<%= @my_grocery.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
